{
  "name": "receiptscan-backend",
  "version": "1.0.0",
  "description": "AI-powered receipt scanning and expense tracking API for receiptscan.ai",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "NODE_ENV=development npm run dev",
    "start:test": "NODE_ENV=test npm run start",
    "start:prod": "NODE_ENV=production npm run start",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "deploy:dev": "./scripts/deploy.sh dev",
    "deploy:test": "./scripts/deploy.sh test",
    "deploy:prd": "./scripts/deploy.sh prd",
    "seed:dev": "./scripts/seed-database.sh dev",
    "seed:test": "./scripts/seed-database.sh test"
  },
  "keywords": [
    "receipt",
    "expense",
    "tracking",
    "ai",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^7.18.0",
    "@types/multer": "^2.0.0",
    "@types/pdfkit": "^0.17.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-winston": "^4.2.0",
    "fast-csv": "^5.0.5",
    "firebase-admin": "^13.6.0",
    "helmet": "^8.1.0",
    "multer": "^2.0.2",
    "openai": "^6.15.0",
    "pdfkit": "^0.17.2",
    "stripe": "^20.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "openapi-types": "^12.1.3",
    "prettier": "^3.7.4",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
